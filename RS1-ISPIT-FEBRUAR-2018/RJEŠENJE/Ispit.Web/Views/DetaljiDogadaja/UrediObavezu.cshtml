@model Korak4
    <form  ajax-poziv="da" ajax-rezultat="ajaxDiv" action="/DetaljiDogadaja/UrediSnimi" method="post">
        <div class="form-group">
            <label>Obaveza :</label>
            <input readonly asp-for="@Model.Naziv" />
            <input hidden asp-for="@Model.Id" />
            <input hidden asp-for="@Model.DogadjajId" />
        </div>
        <div class="form-group">
            <label>Završeno procentualno :</label>
            <input  asp-for="@Model.procentualno" /><span> %</span>
        </div>
        <input type="submit" value="Snimi" />
    </form>