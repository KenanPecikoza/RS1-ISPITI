@model IEnumerable<Korak1Nastavnici> 

    <table class="table table-bordered text-center" >
        <tr>
            <td>Ime</td>
            <td>User name</td>
            <td>Akcija</td>
        </tr>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Ime</td>
                <td>@i.Username</td>
                <td>
                    <a asp-action="Korak2" asp-route-Id="@i.Id" >Izaberi</a>
                </td>
            </tr>
        }
    </table>
